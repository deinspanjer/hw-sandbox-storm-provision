    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>RowProcessor</class_name>

        <class_source><![CDATA[import twitter4j.*;
import twitter4j.auth.*;
import twitter4j.conf.*;

import java.util.ArrayList;
import java.util.Arrays;

private int tickCounter = 0;
private int timeout = 0;
private TwitterStream twitterStream = null;

private StatusListener listener = new StatusListener() {
    public void onStatus(Status status) throws KettleException {
		incrementLinesInput();
        logDebug("@" + status.getUser().getScreenName() + " - " + status.getText());

        Object[] r = createOutputRow(new Object[0], getOutputRowMeta().size());

        int index = 0;

        r[index++] = status.getCreatedAt();
        r[index++] = Long.valueOf(status.getId());
        r[index++] = (status.getInReplyToStatusId()!=-1)?Long.valueOf(status.getInReplyToStatusId()):null;
        r[index++] = status.getText().replace("\n"," ");
        r[index++] = Long.valueOf(status.getUser().getId());
        r[index++] = status.getUser().getScreenName();

        putRow(getOutputRowMeta(), r);    

    }
    public void onDeletionNotice(StatusDeletionNotice statusDeletionNotice) {
		incrementLinesRejected();
        logDebug("Got a status deletion notice id:" + statusDeletionNotice.getStatusId());
    }
    public void onTrackLimitationNotice(int numberOfLimitedStatuses) {
        logDebug("Got track limitation notice:" + numberOfLimitedStatuses);
    }
    public void onScrubGeo(long userId, long upToStatusId) {
        logDebug("Got scrub_geo event userId:" + userId + " upToStatusId:" + upToStatusId);
    }
    public void onStallWarning(StallWarning warning) {
		incrementLinesSkipped();
        logDebug("Got stall warning:" + warning);
    }
    public void onException(Exception ex) {
		if (ex instanceof NullPointerException && ex.getMessage().equals("Inflater has been closed")) {
			// https://groups.google.com/forum/#!topic/twitter4j/XRKR1vBasy0
			logDebug("Got the Inflater finalize bug exception");
		} else {
	        logError("Got an exception:", ex);
			setErrors(1);
		}
    }
};

public RowMetaInterface getOutputRowMeta()
{
	return data.outputRowMeta;
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException 
{
    if (first) {
        first = false;
        twitterStream.addListener(listener);

        // Fire off the stream filter which will repeatedly callback into the listener
        twitterStream.filter(new FilterQuery(0, null, getVariable("FILTERS","obama").split(",")));
        //twitterStream.sample();
		timeout = Integer.valueOf(getVariable("TIMEOUT","15"));
    }

    try {
        Thread.sleep(1000);
		incrementLinesRead();
        if (tickCounter++ >= timeout) {
            logBasic("Finished "+tickCounter+" seconds of streaming");
            setOutputDone();
            return false;
        }
    } catch (InterruptedException e) {
        logError("Caught InterruptedException.");
		setErrors(1);
    }
    return true;
}

public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface) {
    if (super.init(stepMetaInterface, stepDataInterface)) {
		try {
        	twitterStream = new TwitterStreamFactory().getInstance();
	        return true;
		} catch (Exception ex) {
            logError("Got an exception:", ex);
			setErrors(1);
		}
    }
    return false;
}


public void dispose(StepMetaInterface smi, StepDataInterface sdi)
{
    try {
        if (twitterStream != null) {
            logDebug("Stopping stream...");
            twitterStream.shutdown();
            logDebug("Stream is stopped.");
        }
    } catch (Exception e) {
        logError("Error:", e);
        setErrors(1);
    }
    parent.disposeImpl(smi, sdi);
}

]]></class_source>
